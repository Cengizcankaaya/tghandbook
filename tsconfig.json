{
  "compilerOptions": {
    "target": "es2020",
    "module": "nodenext",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@pages/*": [
        "data/pages/*"
      ],
      "~*": [
        "src/*"
      ]
    }
  },
  "include": [
    "vite.config.ts",
  ]
}